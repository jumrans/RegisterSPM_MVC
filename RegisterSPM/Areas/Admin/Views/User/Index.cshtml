@model IEnumerable<Tahun>
@{
  ViewData["Active"] = "User";
}


<div class="mb-3">
  <h3 class="h3 d-inline align-middle">@ViewData["Active"].ToString()</h3>
  <h5 class="badge bg-dark text-white ms-2">Manajemen User App</h5>
</div>
<div class="card">
  <div class="card-header">
    <div class="d-flex flex-row">
      <div class="justify-content-end">
        <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register" asp-route-returnUrl="/Admin/User"><i class="align-middle" data-feather="plus"></i> <span class="align-middle">Tambah</span></a>
      </div>
    </div>
  </div>
  <div class="card-body">
    <div class="row">
      <div class="col-12">
        <table id="tblData" class="display" width="100%">
          <thead>
          <tr>
            <th>User ID</th>
            <th>Email</th>
            <th>Nama</th>
            <th>NIP</th>
            <th>Jabatan</th>
            <th>Role</th>
            <th class="text-center">Action</th>
          </tr>
          </thead>
        </table>
      </div>
    </div>
  </div>
</div>

@section Scripts {
  <script src="~/js/user.js"></script>
}